<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>

<HBox spacing="20" prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controller.PickUpParcelController">

    <!-- Main Content Area -->
    <VBox prefWidth="750" spacing="20">
        
        <!-- Table for displaying parcels -->
        <TableView fx:id="parcelTable" prefWidth="700">
            <columns>
                <TableColumn fx:id="parcelIdColumn" text="Parcel ID" prefWidth="200"/>
                <TableColumn fx:id="statusColumn" text="Status" prefWidth="200"/>
                <TableColumn fx:id="actionColumn" text="Action" prefWidth="300"/>
            </columns>
        </TableView>
    </VBox>
</HBox>
